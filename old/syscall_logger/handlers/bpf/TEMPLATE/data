struct data_t {
    int parent_pid;
    int pid;                            // PID del processo padre (processo corrente)
    int child_pid;                      // PID del processo figlio (per fork/clone)
    int pgid;                           // Process Group ID
    int tid;                            // Thread ID
    int tgid;                           // thread Group ID ??????
    char command[TASK_COMM_LEN];        // Nome del processo padre
    u64 timestamp;                      // Timestamp dell'evento
};